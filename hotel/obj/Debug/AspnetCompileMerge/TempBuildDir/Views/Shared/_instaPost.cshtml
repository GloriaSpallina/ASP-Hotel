<aside class="single_sidebar_widget instagram_feeds">
    <h4 class="widget_title">Instagram Feeds</h4>
    <ul class="instagram_row flex-wrap">
        @foreach (hotel.Models.ArticleModel item in Model)
        {

            <li>
                <a href="@item.LienArticle">
                    <img class="img-fluid" src="/img/post/@item.Photo" alt="">
                </a>
            </li>
        }

    </ul>
</aside>
